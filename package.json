{
	"name": "tack",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "concurrently -n next,jest \"NODE_OPTIONS='--inspect' node --require ./scripts/tracing.js ./node_modules/.bin/next dev\" \"npm:test:watch\"",
		"build": "next build",
		"clean": "rimraf .next .cypress coverage out reports",
		"create:migration": "ts-node ./node_modules/typeorm/cli.js migration:generate -d lib/database/migrations -n",
		"export": "next export",
		"format": "prettier --write .",
		"format:check": "prettier --list-different .",
		"ingest": "ts-node scripts/ingest.ts",
		"lint": "npm run lint:check -- --fix",
		"lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
		"posttest": "concurrently \"type:*\" \"lint:*\" \"format:*\"",
		"preexport": "npm run build",
		"schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
		"schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
		"schema:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
		"seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
		"seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
		"start": "node --require ./scripts/tracing.js ./node_modules/.bin/next start",
		"test": "jest --coverage --verbose --colors",
		"test:watch": "jest --watch",
		"type:check": "tsc --noEmit"
	},
	"bin": {
		"tack": "./bin/run"
	},
	"browserslist": [
		">0.3%",
		"not ie 11",
		"not dead",
		"not op_mini all"
	],
	"eslintConfig": {
		"root": true,
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaFeatures": {
				"jsx": true
			},
			"ecmaVersion": 2018,
			"sourceType": "module"
		},
		"env": {
			"browser": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/eslint-recommended",
			"plugin:@typescript-eslint/recommended",
			"plugin:react/recommended",
			"plugin:jsx-a11y/recommended",
			"prettier/@typescript-eslint",
			"plugin:prettier/recommended"
		],
		"globals": {
			"React": "writable"
		},
		"overrides": [
			{
				"files": [
					"*.js",
					".*.js"
				],
				"rules": {
					"@typescript-eslint/explicit-function-return-type": "off",
					"@typescript-eslint/no-var-requires": "off"
				}
			}
		],
		"rules": {
			"prettier/prettier": [
				"error",
				{},
				{
					"usePrettierrc": true
				}
			],
			"react/prop-types": "off",
			"react/react-in-jsx-scope": "off"
		},
		"settings": {
			"react": {
				"version": "detect"
			}
		}
	},
	"files": [
		"/bin",
		"/lib"
	],
	"lint-staged": {
		"src/**/*.{ts,tsx}": "npm run lint:fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"jest": {
		"collectCoverageFrom": [
			"**/*.{js,jsx,ts,tsx}",
			"!**/*.d.ts",
			"!**/node_modules/**"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/__tests__/jest.setup.js"
		],
		"moduleNameMapper": {
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"testPathIgnorePatterns": [
			"/.cypress/",
			"/.next/",
			"/build/",
			"/docs/",
			"/__e2e__/",
			"/node_modules/",
			"/out/",
			"/public/",
			"/styles/",
			"__tests__/jest.setup.js",
			"__tests__/jest.transform.js"
		],
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
			"^.+\\.css$": "<rootDir>/__tests__/jest.transform.js"
		},
		"transformIgnorePatterns": [
			"/node_modules/",
			"^.+\\.module\\.(css|sass|scss)$"
		]
	},
	"oclif": {
		"commands": "./lib/cli/commands",
		"bin": "tack",
		"dirname": "tack",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-plugins"
		],
		"topicSeparator": " ",
		"topics": {
			"hello": {
				"description": "Say hello to the world and others"
			}
		}
	},
	"prettier": {
		"semi": false,
		"trailingComma": "all",
		"singleQuote": true,
		"useTabs": true
	},
	"dependencies": {
		"@babel/node": "^7.15.8",
		"@babel/register": "^7.15.3",
		"@grpc/grpc-js": "^1.4.2",
		"@oclif/core": "^1",
		"@oclif/plugin-help": "^5",
		"@oclif/plugin-plugins": "^2.0.1",
		"@opentelemetry/api": "^1.0.3",
		"@opentelemetry/auto-instrumentations-node": "^0.26.0",
		"@opentelemetry/exporter-collector-grpc": "^0.25.0",
		"@opentelemetry/sdk-node": "^0.26.0",
		"@stitches/react": "^1.2.5",
		"@xstate/react": "^1.6.1",
		"apollo-server-core": "^3.4.0",
		"apollo-server-micro": "^3.4.0",
		"cache-manager": "^4.1.0",
		"cache-manager-fs-hash": "^1.0.0",
		"chalk": "^4.1.2",
		"class-validator": "^0.14.0",
		"cross-fetch": "^3.1.4",
		"date-fns": "^2.25.0",
		"debug": "^4.3.2",
		"fast-xml-parser": "^3.21.0",
		"fetch-cookie": "^1.0.0",
		"graphql": "^15.7.2",
		"ink": "^3.2.0",
		"micro": "^9.3.4",
		"micro-cors": "^0.1.1",
		"next": "^12.0.1",
		"next-swagger-doc": "^0.1.10",
		"node-html-parser": "^5.1.0",
		"pg": "^8.7.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"reflect-metadata": "^0.1.13",
		"swagger-ui-react": "^3.52.5",
		"type-graphql": "^1.1.1",
		"typeorm": "^0.2.38",
		"xstate": "^4.25.0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-class-properties": "^7.16.0",
		"@babel/plugin-proposal-decorators": "^7.16.0",
		"@babel/plugin-proposal-private-methods": "^7.16.0",
		"@babel/plugin-proposal-private-property-in-object": "^7.16.0",
		"@oclif/test": "^2",
		"@testing-library/cypress": "^8.0.1",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.2",
		"@types/cache-manager": "^4.0.1",
		"@types/cache-manager-fs-hash": "0.0.1",
		"@types/jest": "^27.0.2",
		"@types/micro": "^7.3.6",
		"@types/micro-cors": "^0.1.2",
		"@types/node": "^16.11.6",
		"@types/react": "^17.0.33",
		"@types/swagger-ui-react": "^3.35.3",
		"@types/testing-library__cypress": "^5.0.9",
		"@types/testing-library__react": "^10.0.1",
		"@types/tough-cookie": "^4.0.1",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"babel-jest": "^27.3.1",
		"babel-plugin-transform-typescript-metadata": "^0.3.2",
		"concurrently": "^6.3.0",
		"cypress": "^8.7.0",
		"dotenv-flow": "^3.2.0",
		"eslint": "^8.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.26.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^7.0.4",
		"jest": "^27.3.1",
		"lint-staged": "^11.2.6",
		"prettier": "^2.4.1",
		"tough-cookie": "^4.0.0",
		"ts-node": "^10.4.0",
		"tslib": "^2.3.1",
		"typeorm-seeding": "^1.6.1",
		"typescript": "^4.4.4",
		"ultra-runner": "^3.10.5",
		"vercel": "^23.1.2"
	}
}
